.menuPrincipal * { 
	font-family			: Verdana, Arial,sans-serif;
	font-weight			: bold;
	font-size			: 10px;
	list-style			: none; 
	text-decoration		: none; 
	margin				: 0; 
	padding				: 0; 
}   


.menuPrincipal > li { 
	float				: left;
	width				: 14%; 
	border-left			: 1px solid #ffffff; 
}   

.menuPrincipal li a { 
	color				: #FFF; 
	display				: block;
	padding				: 2px 0px 2px 0px;
	text-align			: center;
}   

.menuPrincipal li a:hover { 
	color				: #000000;
} 

